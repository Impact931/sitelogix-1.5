# SiteLogix Frontend Environment Variables
# Copy this file to .env.local and fill in your values
# NEVER commit .env.local to git!

# API Configuration
VITE_API_BASE_URL=https://your-api-url.execute-api.us-east-1.amazonaws.com/api

# ElevenLabs Voice AI
VITE_ELEVEN_LABS_AGENT_ID=your_agent_id
VITE_ELEVEN_LABS_API_KEY=your_api_key
VITE_ELEVEN_LABS_MODEL=eleven_turbo_v2
VITE_ELEVEN_LABS_VOICE_ID=your_voice_id

# Google OAuth
VITE_GOOGLE_CLIENT_ID=your_client_id.apps.googleusercontent.com
VITE_GOOGLE_WORKSPACE_FOLDER_ID=your_folder_id

# Super Admin Developer Account (PRODUCTION ONLY - Set in Amplify Console)
VITE_SUPER_ADMIN_USERNAME=Jayson Rivas
